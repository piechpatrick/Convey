{
  "app": {
    "name": "Conveyor Documents Service"
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "documents-service",
    "address": "docker.for.win.localhost",
    "port": "5001",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "documents-service"
  },
  "logger": {
    "applicationName": "documents-service",
    "excludePaths": [ "/ping", "/metrics" ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "jaeger": {
    "enabled": false,
    "serviceName": "documents",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": [ "/ping", "/metrics" ]
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5
  },
  "prometheus": {
    "enabled": false
  },
  "rabbitMq": {
    "connectionName": "documents-service",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true,
      "logConnectionStatus": true,
      "logMessagePayload": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "documents"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "{{assembly}}/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    }
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "documents:"
  },
  "seaweed": {
    "enabled": true,
    "filerHttpClientName": "filerHttpClient",
    "filerUrl": "http://localhost:8888",
    "masterHttpClientName": "masterHttpClient",
    "masterUrl": "http://localhost:9333"
  }
}